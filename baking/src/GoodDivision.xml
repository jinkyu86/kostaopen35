<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
	PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
		
<mapper namespace="Good_division">		
	<resultMap type="kr.or.kosta.gooddivision.Good_division" id="selectParameterMap">
		<result property="division"		column="DIVISION"/>		
		<result property="gName"		column="G_NAME"/>
	</resultMap>
	
	<select id="selectGoodDivision"  resultMap="selectParameterMap" >
		SELECT division, g_name FROM good_division
	</select>
	
</mapper>